{
  "AWS": {
    "Region": "eu-north-1"
  },
  "AwsS3Settings": {
    "FileExpireInSeconds" : 600
  },
  "Database": {
    "ConnectoinString": "aws secret"
  },
  "Jwt": {
    "Key": "aws secret",
    "Issuer": "https://johnsen.art",
    "Audience": "https://johnsen.art"
  },
  "Stripe":{
    "SecretKey": "not configured for production yet",
    "WebhookSecret": "not configured for production yet",
    "PublishableKey": "not configured for production yet"
  },
  "Smtp":{
    "Host": "smtp.domeneshop.no",
    "Port": "587",
    "Username": "aws secret",
    "Password": "aws secret",
    "From": "info@johnsen.art"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "indent": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/logs-.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "indent": true
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
